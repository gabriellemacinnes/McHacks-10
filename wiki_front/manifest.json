{
  "manifest_version": 3,

  "name": "Wiki Search",
  "description": "This extension gives you a summary from WikiPedia.",
  "version": "1.0",

  "action": {
    "default_popup": "popup.html",
    "default_title": "Wiki Search"
  },

  "background": {
    "service_worker" : "background.js"
  },

  "permissions": [
		"tabs",
		"background",
        "identity",
        "notifications"
	],


  "host_permissions": ["http://localhost:5000/*"]

}