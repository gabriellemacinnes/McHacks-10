{
  "manifest_version": 3,

  "name": "Hello World",
  "description": "A simple page-scraping extension for Chrome",
  "version": "1.0",

  "action": {
    "default_popup": "popup.html",
    "default_title": "Hello World",
    "default_icon": "logo.png"
  },

  "host_permissions": ["<all_urls>"]
}